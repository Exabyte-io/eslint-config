{
    "name": "@exabyte-io/eslint-config-mat3ra",
    "version": "0.0.0",
    "description": "Eslint configuration extension to be reused across all mat3ra projects.",
    "scripts": {
        "test": "npm run lint",
        "lint": "eslint index.js && prettier --write index.js",
        "lint:fix": "eslint --fix --cache index.js && prettier --write index.js",
        "prettier": "prettier --check index.js",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Exabyte-io/eslint-config-mat3ra.git"
    },
    "main": "index.js",
    "files": [
        "index.js",
        ".babelrc"
    ],
    "author": "Exabyte Inc.",
    "bugs": {
        "url": "https://github.com/Exabyte-io/eslint-config-mat3ra/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Exabyte-io/eslint-config-mat3ra",
    "devDependencies": {
        "@babel/eslint-parser": "7.16.3",
        "@babel/plugin-proposal-class-properties": "7.16.0",
        "@babel/preset-env": "7.16.4",
        "@babel/preset-react": "7.16.7",
        "@babel/register": "^7.16.0",
        "@babel/runtime-corejs3": "7.16.8",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "19.0.2",
        "eslint-import-resolver-exports": "^1.0.0-beta.2",
        "eslint-import-resolver-meteor": "^0.4.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "mocha": "^9.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.7.1"
    },
    "peerDependencies": {
        "@babel/eslint-parser": "7.16.3",
        "@babel/plugin-proposal-class-properties": "7.16.0",
        "@babel/preset-env": "7.16.4",
        "@babel/preset-react": "7.16.7",
        "@babel/register": "^7.16.0",
        "@babel/runtime-corejs3": "7.16.8",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "19.0.2",
        "eslint-import-resolver-exports": "^1.0.0-beta.2",
        "eslint-import-resolver-meteor": "^0.4.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-simple-import-sort": "7.0.0"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css}": "prettier --write"
    }
}
